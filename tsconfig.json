{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@shared/*": ["./shared/*"],
      "@modules/*": ["./modules/*"],
    }
  },
  "include": ["src", "prisma"]
}
